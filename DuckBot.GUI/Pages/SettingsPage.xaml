<Page x:Class="DuckBot.GUI.Pages.SettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="Settings">
    <StackPanel Margin="10">
        <TextBlock Text="Logged in as:" FontWeight="Bold"/>
        <TextBlock x:Name="UserLabel" Margin="5"/>
        <TextBlock Text="Subscription:" FontWeight="Bold" Margin="0,10,0,0"/>
        <TextBlock x:Name="SubLabel" Margin="5"/>
    </StackPanel>
</Page>
